solutionPath=QuickApp.sln
dockerFile=.docker/Dockerfile.webapi
buildTech=dotnet
buildContainer=huyntt/jenkins-agent
dockerBuildContainer=docker:dind
msBuildVersion=dotnet
email: support@devopsify.co
createRelease=false
releaseBranch=master
BuildStep1_WebAPI=dotnet restore QuickApp.sln
BuildStep2_WebAPI=dotnet build QuickApp.sln --configuration Release
BuildStep3_WebApp=npm install --force --prefix QuickApp/ClientApp
BuildStep4_WebApp=npm run --prefix QuickApp/ClientApp build
TestUnitTesting=dotnet test QuickApp.Tests/QuickApp.Tests.csproj -o target
UploadPublish=dotnet publish -c Release -o published
createImage=true
runJobs=false
runJobsList= Demos/DevOpsify/demo-testpipeline/master
Demos/DevOpsify/demo-testpipeline/masterWait = false
#Demos/testJob1Param1 = string, stringParam, This is from buildOption string
#Demos/testJob1Param2 = booleanParam, booleanParam, false
#Demos/testJob1Wait = false
sonarProjectKey=demo-QuickApp
dockerHubCredential=DockerHub
baseVersion=1.0
appName=demo-quickapp-api
appVersion=latest
